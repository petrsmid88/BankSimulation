<Window x:Class="Bank.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bank"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid x:Name="AdminWindow1">
        <Menu HorizontalAlignment="Left" Height="95" VerticalAlignment="Top" Width="782">
            <MenuItem x:Name="AdminMenu" Header="Acount" Height="25" Width="105">
                <MenuItem Header="Change password" Background="{x:Null}" Click="ChangePassword_Click"/>
            </MenuItem>
        </Menu>

    </Grid>
</Window>
